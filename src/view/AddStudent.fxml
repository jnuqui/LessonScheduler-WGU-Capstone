        <TextField fx:id="phoneTextField" layoutX="152.0" layoutY="177.0" />
        <Button layoutX="167.0" layoutY="240.0" mnemonicParsing="false" onAction="#insertStudent" text="Save" />
        <Button layoutX="227.0" layoutY="240.0" mnemonicParsing="false" onAction="#resetFields" text="Reset" />
